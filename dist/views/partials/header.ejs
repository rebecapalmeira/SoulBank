    <script>
      $(document).ready(function () {
          // configuração botão de seta para cima no scroll
          var janela = $(window);

          janela.on('scroll', function () {
          if (janela.scrollTop() < "400") {
              $("#setaParaCima").css({ visibility: 'hidden' });
          } else {
              $("#setaParaCima").css({ visibility: 'visible', scrollTop: 'smooth' });
          }
          })

          $("#setaParaCima").click(function (){
          $('html').animate({scrollTop: '0px'}, 600);
          });

      });

      // botão de libras
      new window.VLibras.Widget('https://vlibras.gov.br/app');
        
    </script>



